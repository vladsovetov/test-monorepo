{
  "name": "@vladsovetov/admin360",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "Vlad Sovetov <vlad.sovetov@gmail.com>",
  "homepage": "https://github.com/vladsovetov/test-monorepo#readme",
  "license": "ISC",
  "main": "lib/admin360.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": "git://github.com/vladsovetov/test-monorepo.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "release": {
    "extends": "semantic-release-monorepo",
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "assets": [
            "dist/**/*.{js,css}",
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/vladsovetov/test-monorepo/issues"
  },
  "dependencies": {
    "@vladsovetov/shared": "0.0.0"
  },
  "devDependencies": {
    "semantic-release": "^19.0.3",
    "semantic-release-monorepo": "^7.0.5"
  }
}
